{
  "name": "TrustChainNetwork",
  "version": "1.0.0",
  "client": {
    "organization": "LTO",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "LTO": {
      "mspid": "LTOMSP",
      "peers": [
        "peer0.lto.gov.ph"
      ],
      "certificateAuthorities": [
        "ca.lto.gov.ph"
      ]
    }
  },
  "peers": {
    "peer0.lto.gov.ph": {
      "url": "grpcs://localhost:7051",
      "grpcOptions": {
        "ssl-target-name-override": "peer0.lto.gov.ph",
        "grpc.keepalive_time_ms": 600000
      },
      "tlsCACerts": {
        "path": "fabric-network/crypto-config/peerOrganizations/lto.gov.ph/peers/peer0.lto.gov.ph/tls/ca.crt"
      }
    }
  },
  "certificateAuthorities": {
    "ca.lto.gov.ph": {
      "url": "https://localhost:7054",
      "caName": "ca.lto.gov.ph",
      "tlsCACerts": {
        "path": "fabric-network/crypto-config/peerOrganizations/lto.gov.ph/ca/ca.lto.gov.ph-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    }
  },
  "orderers": {
    "orderer.lto.gov.ph": {
      "url": "grpcs://localhost:7050",
      "grpcOptions": {
        "ssl-target-name-override": "orderer.lto.gov.ph",
        "grpc.keepalive_time_ms": 600000
      },
      "tlsCACerts": {
        "path": "fabric-network/crypto-config/ordererOrganizations/lto.gov.ph/orderers/orderer.lto.gov.ph/tls/ca.crt"
      }
    }
  },
  "channels": {
    "ltochannel": {
      "orderers": [
        "orderer.lto.gov.ph"
      ],
      "peers": {
        "peer0.lto.gov.ph": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      }
    }
  }
}

